{
  "name": "@atlauncher/discord-bot",
  "version": "0.1.0",
  "description": "This is the code for our Discord bot which runs on our official Discord server at https://atl.pw/discord",
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Discord-Bot.git"
  },
  "keywords": [
    "discord",
    "atlauncher",
    "bot"
  ],
  "author": "Ryan Dowling <ryan.dowling@atlauncher.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Discord-Bot/issues"
  },
  "homepage": "https://github.com/ATLauncher/Discord-Bot#readme",
  "scripts": {
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "config:base64": "babel-node utils/base64Config.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "format": "prettier --write \"src/**/*.js\"",
    "oauth:url": "babel-node utils/getOAuthUrl.js",
    "start": "node dist"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@sentry/node": "^5.6.1",
    "aws-sdk": "^2.512.0",
    "config": "^3.2.2",
    "date-fns": "^1.30.1",
    "discord.js": "^11.5.1",
    "nedb": "^1.8.0",
    "nedb-promise": "^2.0.1",
    "winston": "^3.2.1",
    "winston-logzio": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "cross-env": "^5.2.0",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0"
  }
}
