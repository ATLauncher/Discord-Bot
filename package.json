{
  "name": "@atlauncher/discord-bot",
  "version": "0.1.0",
  "description": "This is the code for our Discord bot which runs on our official Discord server at https://atl.pw/discord",
  "main": "dist",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATLauncher/Discord-Bot.git"
  },
  "keywords": [
    "discord",
    "atlauncher",
    "bot"
  ],
  "author": "Ryan Dowling <ryan.dowling@atlauncher.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ATLauncher/Discord-Bot/issues"
  },
  "homepage": "https://github.com/ATLauncher/Discord-Bot#readme",
  "scripts": {
    "build": "npm run generate:prisma && tsc",
    "clean": "rimraf dist",
    "config:base64": "ts-node utils/stringifyConfig.ts",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:save": "prisma migrate dev",
    "dev": "cross-env NODE_ENV=development nodemon",
    "generate:prisma": "prisma generate",
    "oauth:url": "ts-node utils/getOAuthUrl.ts",
    "postinstall": "patch-package",
    "start": "node dist",
    "test": "echo 'Error: no tests written :( bad dev'"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,utils}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@koa/router": "^10.0.0",
    "@prisma/client": "^2.20.1",
    "@sentry/node": "^6.1.0",
    "config": "^3.3.3",
    "date-fns": "^2.16.1",
    "discord.js": "^12.5.1",
    "got": "^11.8.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "node-schedule": "^2.0.0",
    "pluralize": "^8.0.0",
    "winston": "^3.3.3",
    "winston-logzio": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/config": "0.0.38",
    "@types/koa": "^2.11.7",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^14.14.25",
    "@types/node-schedule": "^1.3.1",
    "@types/pluralize": "0.0.29",
    "@types/ws": "^7.4.0",
    "cross-env": "^7.0.3",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "patch-package": "^6.2.2",
    "prettier": "^2.2.1",
    "prisma": "^2.20.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
